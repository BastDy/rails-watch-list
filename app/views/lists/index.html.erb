<h1 class="text-center">My list</h1>


<div class="container d-flex flex-column">
  <% @lists.each do |list| %>
    <div class="card text-center">
      <h2><%= link_to list.name, list_path(list) %></h2>
    </div>
  <% end %>
  <button class="btn"><%= link_to "Create a List", new_list_path%></button>
</div>
